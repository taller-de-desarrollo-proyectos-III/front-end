{
  "name": "front-end",
  "version": "0.1.0",
  "contributors": [
    "Sebastian Blanco <sebastian.e.blanco@gmail.com>",
    "Nerea Piccone <nerenap@gmail.com>",
    "Sebastian Leonardo Perez <sebastian.leo.perez@gmail.com>",
    "Lautaro De Los Heros <lautarodelosheros@gmail.com>"
  ],
  "engines": {
    "node": "12.14.0"
  },
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "@types/classnames": "2.2.10",
    "@types/jest": "24.0.0",
    "@types/node": "12.0.0",
    "@types/node-sass": "4.11.1",
    "@types/react": "16.9.0",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "5.1.6",
    "classnames": "2.2.6",
    "link-module-alias": "1.2.0",
    "node-sass": "4.14.1",
    "normalize.css": "8.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "typescript": "3.7.2",
    "typescript-logging": "^1.0.0"
  },
  "devDependencies": {
    "husky": "4.3.0",
    "stylelint": "13.7.2",
    "stylelint-config-sass-guidelines": "7.1.0",
    "stylelint-config-standard": "20.0.0",
    "tslint": "6.1.3",
    "tslint-react-hooks": "2.2.2"
  },
  "scripts": {
    "start": "PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test",
    "eject": "react-scripts eject",
    "preinstall": "which link-module-alias && link-module-alias clean || echo",
    "postinstall": "link-module-alias",
    "lint": "yarn lint:src:ts && yarn lint:src:tsx && yarn lint:test:ts && yarn lint:test:tsx && yarn build:lint && yarn lint:scss",
    "lint:src:ts": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:src:tsx": "tslint -c tslint.json 'src/**/*.tsx'",
    "lint:test:ts": "tslint -c tslint.json 'test/**/*.ts'",
    "lint:test:tsx": "tslint -c tslint.json 'test/**/*.tsx'",
    "build:lint": "tsc --noEmit -p .",
    "lint:scss": "yarn stylelint '**/*.scss'"
  },
  "_moduleAliases": {
    "$src": "./src",
    "$styles": "./src/styles",
    "$components": "./src/components",
    "$pages": "./src/pages"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  }
}
